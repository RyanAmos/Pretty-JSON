{
	"name": "Pretty JSON",
	"version": "0.1",
	"description": "Pretty print JSON to the browser for increased readability.",
	"default_icon": "pretty_json.png",
	"content_scripts": [ {
		"css": [ "pretty_json.css" ],
		"js": [ "pretty_json.js" ],
		"matches": [ "http://*/*.json", "https://*/*.json", "http://*/*.json?*", "https://*/*.json?*", "http://*/*?*=json", "https://*/*?*=json", "http://*/*?*=json&*", "https://*/*?*=json&*", "http://*/*?json", "https://*/*?json", "http://*/*?json&*", "https://*/*?json&*" ]
	} ],
	"version": "0.1",
	"icons": {
		"32": "icon_32x32.png",
		"48": "icon_48x48.png",
		"128": "icon_128x128.png"
	}
}